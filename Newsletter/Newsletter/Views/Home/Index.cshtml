
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Subscriptions</h2>

@if(Model != null)
{   
    foreach(var subscription in Model)
    {
        <li><a href=@Url.Action("Details", "Subscriptions", new { id = @subscription.ID })>@subscription.Name</a></li>
    }
}

@Html.ActionLink("Sign me up!", "Create", "Subscriptions")





